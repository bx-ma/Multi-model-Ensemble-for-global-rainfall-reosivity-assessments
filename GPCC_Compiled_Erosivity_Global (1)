var EQ_16 = function(img1){
    var MFI = img1;
    var results = MFI.pow(1.205).multiply(5.249).set('year', year);
    return  results;
};
var R_era5_m=EQ_16(MFI_GP).rename('ChinaChongqing_EQ16_gpcc');
var ChinaChongqing_EQ16 = ee.Image.cat([R_era5_m]).clip(ChinaChongqing);

